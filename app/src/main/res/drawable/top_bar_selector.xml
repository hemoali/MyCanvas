        <shape>
            <gradient
                android:angle="90"
                android:endColor="#006666"
                android:startColor="#686b6b"
                android:type="linear"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient
                android:angle="90"
                android:endColor="#006666"
                android:startColor="#686b6b"
                android:type="linear"/>
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="90"
                android:endColor="#0a054d"
                android:startColor="#0a054d"
                android:type="linear"/>
        </shape>
    </item>
</selector>